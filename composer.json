{
    "name": "ambengers/query-filter",
    "description": "Laravel package for filtering resources with request query string",
    "keywords": [
        "query",
        "filter",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marvin Quezon",
            "email": "quezon.marvin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^6.0",
        "laravel/legacy-factories": "^1.0.4"
    },
    "autoload": {
        "psr-4": {
            "Ambengers\\QueryFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ambengers\\QueryFilter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ambengers\\QueryFilter\\QueryFilterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
